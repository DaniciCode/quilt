<div class="d-flex justify-content-center">
  <div class="container-new">
    <h3 class="text-center">Create a memory</h3>
     <%= simple_form_for [@memory],url:memories_path do |f| %>
     <div class="form-inputs">
        <%= f.input :title %>
        <%= f.input :date %>
        <%= f.input :location %>
        <%= f.input :description %>
        <%= f.input :photo, as: :file  %>
      </div>
      <div class="form-actions">
        <%= f.button :submit, "Submit" %>
      </div>
      <% end %>
</div>

</div>
