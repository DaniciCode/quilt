<div class="card-group">
  <%@families.each do |family| %>
  <%= link_to family_path(family) do %>
  <div class="card card-img-top">
    <% if family.photo.attached? %>
    <%=cl_image_tag family.photo.key, width: 300   %>
    <% end %>
  <div class="card-body">
    <h5 class="card-title"><%=family.family_name%></h5>
    <p class="card-text"><%=family.description%></p>
    <p class="card-text"><%= link_to "Create new family", new_family_path %></p>
    <%end%>
    <%end%>
  </div>
  </div>
</div>




  <%= link_to "Edit Family" %>
